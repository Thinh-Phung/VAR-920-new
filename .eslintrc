{
  "extends": ["react-app", "prettier", "airbnb", "airbnb/hooks"],
  "plugins": ["react-app", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true,

        "semi": false,
        "trailingComma": "none",
        "endOfLine": "lf",
        "tabWidth": 2,
        "useTabs": false,
        "printWidth": 120,
        "jsxSingleQuote": false
      }
    ]
  }
}
